{
  "name": "guidooffermans/kirbycms",
  "description": "Kirby cms",
  "type": "project",
  "keywords": [
    "kirby",
    "cms"
  ],
  "authors": [
    {
      "name": "Guido Offermans"
    }
  ],
  "require": {
    "php": ">=8.1.0 <8.2.0",
    "getkirby/cms": "^3.9.2"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ]
  }
}
